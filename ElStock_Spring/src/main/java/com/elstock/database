##################### 크롤링 이후, 선제적으로 해야할 sql 구문들

delete from fss where dates = '2022.12(E)' ;
delete from companys where company_rank ='-위';
alter table companys modify company_rank int;

select * from companys;
select * from markets;
select * from news;
select * from members;

alter table markets drop volumn;

select * from fss;

select * from companys order by company_rank asc;
drop table companys;



#### 뉴스 칼럼 추가
create table news(
   no int not null auto_increment primary key,
    news_url varchar(2000),
    news_header varchar(2000),
    news_body varchar(2000),
    news_img varchar(2000)
);

